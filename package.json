{
  "name": "node-postgre-api",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:codeshifu/node-postgre-api.git",
  "author": "Olushi Luqman Opemipo <olushilukmon03@gmail.com> (https://codeshifu.github.io/)",
  "license": "MIT",
  "scripts": {
    "build": "babel src -d dist",
    "start": "npm run build && node dist/server.js",
    "dev:build": "nodemon -L src --exec babel --presets env -w src/ --out-dir dist/",
    "dev:watch": "nodemon -L dist/server.js",
    "dev": "npm-run-all --parallel 'dev:build' 'dev:watch'"
  },
  "dependencies": {
    "body-parser": "1.18.3",
    "cors": "2.8.4",
    "dotenv": "6.0.0",
    "express": "4.16.3",
    "morgan": "1.9.1",
    "pg": "7.4.3"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-register": "6.26.0",
    "chai": "4.1.2",
    "chai-http": "4.2.0",
    "eslint": "5.6.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.14.0",
    "mocha": "5.2.0",
    "nodemon": "1.18.4",
    "npm-run-all": "4.1.3"
  }
}
